{
  "name" : "bapcat/tailor",
  "license" : "GPL-3.0-or-later",
  "homepage" : "https://github.com/BapCat/Tailor",
  "authors" : [{
      "name" : "Corey Frenette",
      "email" : "corey@narwhunderful.com"
    }
  ],
  "config": {
    "preferred-install": "dist",
    "platform": {
      "php": "7.4"
    },
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "require" : {
    "php" : "^7.4|^8.0",
    "ext-json": "*",
    "bapcat/persist" : "^0.4",
    "bapcat/nom" : "^0.5",
    "bapcat/hashing" : "^0.4"
  },
  "require-dev" : {
    "phpunit/phpunit" : "^9.5"
  },
  "autoload" : {
    "psr-4" : {
      "BapCat\\Tailor\\" : "src/"
    }
  }
}
